<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ClockViewer">
        <attr name="valueHour" format="integer" />
        <attr name="valueMinute" format="integer" />
    </declare-styleable>

    <declare-styleable name="ClockEditView">
        <attr name="actionType" format="integer">
            <flag name="none" value="0x00" />
            <flag name="replace" value="0x01" />
            <flag name="delete" value="0x02" />
            <flag name="ring_enabled" value="0x03" />
            <flag name="ring_disabled" value="0x04" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ClockPatternCheckbox">
        <attr name="checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="UsefulButton">
        <attr name="backgroundDrawable" format="integer" />
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
    </declare-styleable>
</resources>